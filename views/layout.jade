doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		title #{pageTitle} - #{currentPage.title}
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
		meta(rel="shortcut icon" href="#{urlStaticRelative}images/favicons/favicon.ico" type="image/x-icon")
		meta(rel="apple-touch-icon" href="#{urlStaticRelative}images/favicons/apple-touch-icon.png")
		meta(rel="apple-touch-icon" sizes="57x57" href="#{urlStaticRelative}images/favicons/apple-touch-icon-57x57.png")
		meta(rel="apple-touch-icon" sizes="72x72" href="#{urlStaticRelative}images/favicons/apple-touch-icon-72x72.png")
		meta(rel="apple-touch-icon" sizes="76x76" href="#{urlStaticRelative}images/favicons/apple-touch-icon-76x76.png")
		meta(rel="apple-touch-icon" sizes="114x114" href="#{urlStaticRelative}images/favicons/apple-touch-icon-114x114.png")
		meta(rel="apple-touch-icon" sizes="120x120" href="#{urlStaticRelative}images/favicons/apple-touch-icon-120x120.png")
		meta(rel="apple-touch-icon" sizes="144x144" href="#{urlStaticRelative}images/favicons/apple-touch-icon-144x144.png")
		meta(rel="apple-touch-icon" sizes="152x152" href="#{urlStaticRelative}images/favicons/apple-touch-icon-152x152.png")
		meta(name="application-name" content="#{appFullName}")
		meta(name="msapplication-TileColor" content="#F3F3F1")
		meta(name="msapplication-TileImage" content="#{urlStaticRelative}images/favicons/windows-8-tile.png")
		meta(name="format-detection" content="telephone=no")
		link(rel="stylesheet" href="#{urlStaticRelative}styles/#{appNameLowercase}.min.css")
		meta(name="description" content="#{appDescription}")
		meta(name="keywords" content="#{appKeywords}")
		link(rel="canonical" href="#{canonicalUrl}")
		link(href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css')
		script(type="text/javascript").
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
			ga('create', 'UA-80280272-1', 'auto');
			ga('send', 'pageview');
	body(itemscope itemtype="http://schema.org/WebPage")
		div.pageWrapper
			include partials/header.jade
			div.bodyWrapper
				div.bodyBackground(class="#{currentPage.backgroundClass}" bg="#{currentPage.backgroundClass}")
					img(class="bgImg compliance" src="#{urlStaticRelative}images/backgrounds/compliance.jpg")
					img(class="bgImg roof" src="#{urlStaticRelative}images/backgrounds/roof.jpg")
					img(class="bgImg structural" src="#{urlStaticRelative}images/backgrounds/structural.jpg")
					img(class="bgImg telecoms" src="#{urlStaticRelative}images/backgrounds/telecoms.jpg")
				include partials/notifications.jade
				div.obscurer
				div.contentWrapper
					div.bodyContent
						block content
			include partials/footer.jade
		script(type="text/javascript").
			!{labJSScript}
			var ace = { props: !{JSON.stringify(jsProps)}, currentPage: !{JSON.stringify(currentPage)} };
			var externalJS = '#{urlStaticRelative}scripts/#{appNameLowercase}-external.min.js';
			var internalJS = '#{urlStaticRelative}scripts/#{appNameLowercase}-internal.min.js';
			var start = function () { $(document).ready(ace.init); };
			$LAB.script(externalJS).wait().script(internalJS).wait(start);
